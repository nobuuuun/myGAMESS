 $contrl scftyp=rhf runtyp=gradient relwfn=LUT-IOTC ispher=1 $end
 $system mwords=35 $end
 $guess  guess=huckel $end
 $scf    dirscf=.true. $end
 $basis  gbasis=SPKrTZC $end
 $statpt opttol=1d-5 $end
 $data
