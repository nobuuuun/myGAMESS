 $contrl scftyp=rhf dfttyp=b3lyp runtyp=gradient icharg=-1 nzvar=15 $end
 $system mwords=5 $end
 $basis  gbasis=N31 NGAUSS=6 NDFUNC=1 $end
 $zmat   auto=.t. dlc=.t. $end
