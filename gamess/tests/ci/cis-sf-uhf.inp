 $GUESS GUESS=HUCKEL $END 
 $BASIS GBASIS=N21 NGAUSS=3 $END
 $CIS   NSTATE=5 IROOT=1 MULT=1 $END
 $TDDFT NSTATE=5 IROOT=1 MULT=1 $END
