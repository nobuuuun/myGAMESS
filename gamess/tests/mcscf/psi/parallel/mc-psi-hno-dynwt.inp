 $contrl scftyp=mcscf mult=1 runtyp=gradient nzvar=3 $end
 $system parall=.true. mwords=30 memddi=20 $end
 $basis  gbasis=n31 ngauss=6 ndfunc=0 $end
 $zmat   izmat(1)= 1,1,2,  1,2,3,  2,1,2,3 $end
 $statpt opttol=1d-5 $end
!
!     Use VVOS=.TRUE. at the time of RHF to get good starting orbitals!
!     Move both a'' to the very end of active space, to create the QCAS.
 $guess  guess=moread norb=20 norder=1 iorder(3)=3,4,5,6,8,10,11, 7,9 $end
!
!     S0 ground state is ...(pi)2,(n)2,(pi*)0... with S1 being n-->pi*.
!     If the ORMAS space separates a' from a'' orbitals, we have to
!     allow one electron to hop into a'' to generate S1's determinants.
!     There's 5,194 determinants in this ORMAS.
!
!     Use NSTATE high enough to ensure the T1 state between S0 and S1
!     doesn't goof us up in finding at least two singlets.
!
!     Apply dynamic state averaging based on S0/S1 energy separation.
!     WSTATE input marks only those states to be weighted, as IDWREF
!     requests dynamic assignment of weights based on state separation.
 $mcscf  cistep=ormas fullnr=.true. $end
 $det    group=c1 ncore=2 nact=9 nels=12 nstate=4 nhgss=1000
         iroot=1 wstate(1)=1,1,0 idwref=1 $end
 $ormas  nspace=2 mstart(1)=3,10 mine(1)= 9,2 maxe(1)=10,3 $end
!     use the most robust CPMCHF response solver
 $cpmchf gcrodr=.t. micit=30 kicit=10 reclin=.false. $end

