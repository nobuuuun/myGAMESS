      E(3) = 0.236559D+00
      C(3) = 0.647752D+00
      RETURN
C
C           HELIUM
C
C          3  s
  200 CONTINUE
      E(1) = 13.626736D+00
      C(1) = 0.080241D+00
      E(2) = 1.999349D+00
      C(2) = 0.409143D+00
      E(3) = 0.382993D+00
      C(3) = 0.657278D+00
      RETURN
      END SUBROUTINE
